{
  "opts": {
    "encoding": "utf8",
    "destination": "docs/",
    "readme": "readme.md",
    "recurse": true,
    "verbose": true,
    "template": "./node_modules/better-docs"
  },
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    // TODO Edit for all folders where is JSDoc
    "include": [
      "./modules/inventory/my-listings",
      "./components/buttons",
      "./modules/settings/components/UserTable",
      "./components/Confirmable",
      "./modules/add-bank-accounts",
      "./styles",
      "./modules/settings/components/Locations",
      "./modules/settings/components/TradeCriteria",
      "./components/date-format"
    ],
    "exclude": ["./modules/inventory/my-listings/components"]
  },
  "plugins": ["plugins/markdown", "better-docs/category", "better-docs/component"],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "search": true,
    "better-docs": {
      "name": "Echo System Documentation - Frontend",
      "logo": "./assets/images/login/logo_login.png",
      "title": "Echo System Documentation - Frontend"
    }
  }
}
