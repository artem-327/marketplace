{
  "printWidth": 120,
  "singleQuote": true,
  "jsxBracketSameLine": true,
  "semi": false,
  "jsxSingleQuote": true,
  "tabWidth": 2,
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "overrides": [
    {
      "files": [".prettierrc", ".babelrc", ".eslintrc"],
      "options": { "parser": "json" }
    }
  ],
  "trailingComma": "none"
}
